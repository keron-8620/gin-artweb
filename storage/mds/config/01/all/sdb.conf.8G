############################################################
# 配置文件说明
############################################################
# ----------------------------------------------------------
# 1. 支持的长度/数量配置单位
# ----------------------------------------------------------
# 1B = 1 Byte               = 1
# 1K = 1024                 = 1,024
# 1M = 1024 * 1024          = 1,048,576
# 1G = 1024 * 1024 * 1024   = 1,073,741,824
# 1W = 10000                = 10,000
#
# ----------------------------------------------------------
# 2. 数据表类型
# ----------------------------------------------------------
# table.type = Array
# table.type = MvccArray
# table.type = Queue
# table.type = RingBuf
# table.type = PagePool
# table.type = SlotList
# table.type = HashMap
# table.type = MvccMap
# table.type = Raw
#
# ----------------------------------------------------------
# 3. 索引类型
# ----------------------------------------------------------
# index.type = Hash                     # 推荐使用 DHash
# index.type = DHash
# index.type = TTree                    # 推荐使用 RBTree
# index.type = RBTree
# index.type = RBTree_Set
# index.type = Seq
# index.type = Seq_Set
#
# ----------------------------------------------------------
# 4. 操作标志
# ----------------------------------------------------------
# opFlag.READONLY = true                # 只读标志, 默认为可写
# opFlag.ENABLE_VAR = true              # 启用变长数据支持, 默认不支持变长数据
# opFlag.SYNC_READ = true               # 启用同步的读操作, 默认对读操作不加锁
# opFlag.UNSYNC_WRITE = true            # 使用非同步的写操作, 默认对写操作加锁
# opFlag.BLOCK_ON_FULL = true           # 当队列已满时, 阻塞等待
# opFlag.BLOCK_ON_EMPTY = true          # 当队列已空时, 阻塞等待
# opFlag.OVER_WRITE_ON_FULL = true      # 当队列已满时, 覆盖末尾元素
# opFlag.OVER_WRITE_WITH_CHECKER = true # 当队列已满时, 根据检查函数覆盖末尾元素
# opFlag.BUSY_WAITING_ABLE = true       # 当需要阻塞等待时是否进行忙等待, 默认为关闭
# opFlag.DISABLE_POP_METHOD = true      # 禁用队列的Pop()方法
# opFlag.DISABLE_POLL_METHOD = true     # 禁用队列的Poll()方法
# opFlag.DISABLE_ITERATOR_BLOCK = true  # 禁用迭代器的阻塞方法, 默认允许BlockingNext()方法进行阻塞等待
# opFlag.ITERATOR_BLOCK_TIMEOUT = 10000 # 迭代器阻塞等待的超时时间 (微秒), 默认为50000微秒
# opFlag.COND_BLOCK_TIMEOUT = 10000     # 条件变量阻塞等待的超时时间 (微秒), 默认为50000微秒
#
# ----------------------------------------------------------
# 5. 大小写说明
# ----------------------------------------------------------
# 1) 区段名称, 形如 "[SHM_TEST3.table]" 或者 "[SHM_TEST3.tables.1]", 区分大小写
# 2) 配置项关键字, 如 "table.name = ..." 区分大小写
# 3) 配置项值, 如 "table.name = OC_SET201" 不区分大小写
