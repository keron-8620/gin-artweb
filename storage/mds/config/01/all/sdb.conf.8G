#
# 主存库配置文件, 用于统一配置系统使用到的共享内存
# - 使用 8G 内存, 适用于物理内存大于等于 32G 的环境, 预期大页内存: 20G (hugepages=10240)
# - 开启订单簿重建功能需额外占用 15G 内存, 否则无法支持订单簿重建功能 (@note 该容量无法支撑实盘全天的行情数据, 只能用于测试环境)
# - 该文件在集群范围内保持一致即可, 不需要分别为主/备/仲裁节点配置不同的内容
# - @note 启用订单簿重建功能建议配置大于等于 128G 的物理内存, 并使用 40G 的 SDB 配置
#

############################################################
# 总体配置 （共享内存列表）
############################################################

[sdb]
spaces.1 = Model,       0x51${@@}
spaces.2 = TxLog,       0x52${@@}
spaces.3 = TickCache,   0x53${@@}
spaces.4 = OrderBook,   0x54${@@}


############################################################
# Model（领域模型）共享内存配置
############################################################

[Model]
space.maxSize = 6G
space.maxTableNumber = ${IS_ARBITER:!32}${IS_ARBITER:+4}
space.maxIndexNumber = 32
space.useHugePage = ${IS_ARBITER:!${ENABLE_SDB_HUGEPAGE:!no}${ENABLE_SDB_HUGEPAGE:+yes}}${IS_ARBITER:+no}
#space.cpuset = ${NUMA_NODE0_CPU0}


